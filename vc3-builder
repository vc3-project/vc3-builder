    "lsb-exe-distribution":{
        "type":"operating-system-distribution",
        "versions":[
            {
                "version":"v1.0.0",
                "source":{
                    "prerequisites":[
                        "which lsb_release"
                    ],
                    "recipe":[
                        "distro=$(lsb_release -is | tr '[A-Z]' '[a-z]')",
                        "version=$(lsb_release -rs | sed -n -r -e 's/([0-9]+(\\.[0-9]+){0,2}).*/\\1/p')",
                        "[ -n \"${distro}\"  ] || exit 1",
                        "[ -n \"${version}\" ] || exit 1",
                        "echo VC3_MACHINE_DISTRIBUTION: ${distro} ${version}"
                    ]
                }
            }
        ]
    },

